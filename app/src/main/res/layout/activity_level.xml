<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools" android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_gravity="center"
    tools:context="com.example.cash.sumgame.Level">




    <Button   android:id="@+id/NewGame" android:text="Facile"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" android:onClick="Onclickfacile"  />
    <Button   android:id="@+id/ButtAboutgame" android:text="Moyen"
        android:layout_width="match_parent"
        android:layout_height="wrap_content" android:onClick="Onclickmoyen"  />

    <Button   android:id="@+id/ExitGame" android:text="Hard"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"  android:onClick="Onclickhard" />









</LinearLayout>
